            <field name="bank_account_code_prefix">1002</field>
            <field name="transfer_account_id" ref="account_1003" />
        </record>
        <record id="account_1003" model="account.account.template">
            <field name="chart_template_id" ref="l10n_cn_standard" />
        </record>
